<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000"
        motion:autoTransition="animateToEnd">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/tvLogo"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="80dp"
            android:visibility="visible"
            android:alpha="0" />
        <Constraint
            android:id="@+id/ivVector"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="200dp"
            android:layout_height="200dp"
            motion:layout_constraintTop_toBottomOf="@id/tvLogo"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="40dp" />
        <Constraint
            android:id="@+id/ivVector"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="200dp"
            android:layout_height="200dp"
            motion:layout_constraintTop_toBottomOf="@id/tvLogo"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="40dp"
            android:visibility="gone" />
        <Constraint
            android:id="@+id/ivVector"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="200dp"
            android:layout_height="200dp"
            motion:layout_constraintTop_toBottomOf="@id/tvLogo"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="40dp"
            android:visibility="gone" />
        <Constraint
            android:id="@+id/tvTitle"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@id/ivVector"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="40dp" />
        <Constraint
            android:id="@+id/tvTitle"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@id/ivVector"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="40dp"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/tvTitle"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@id/ivVector"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="40dp"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/tvSubtitle"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@id/tvTitle"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="16dp" />
        <Constraint
            android:id="@+id/tvSubtitle"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@id/tvTitle"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="16dp"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/tvSubtitle"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@id/tvTitle"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="16dp"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/btnGettingStarted"
            android:elevation="2dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="32dp"
            motion:layout_constraintTop_toBottomOf="@id/tvSubtitle" />
        <Constraint
            android:id="@+id/btnGettingStarted"
            android:elevation="2dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@id/tvSubtitle"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="32dp"
            android:visibility="gone" />
        <Constraint
            android:id="@+id/btnGettingStarted"
            android:elevation="2dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@id/tvSubtitle"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="32dp"
            android:visibility="gone" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/tvLogo"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="80dp"
            android:alpha="1" />
        <Constraint
            android:id="@+id/ivVector"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="200dp"
            android:layout_height="200dp"
            motion:layout_constraintTop_toBottomOf="@id/tvLogo"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="40dp" />
        <Constraint
            android:id="@+id/ivVector"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="200dp"
            android:layout_height="200dp"
            motion:layout_constraintTop_toBottomOf="@id/tvLogo"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="40dp"
            android:visibility="visible" />
        <Constraint
            android:id="@+id/ivVector"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="200dp"
            android:layout_height="200dp"
            motion:layout_constraintTop_toBottomOf="@id/tvLogo"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="40dp"
            android:visibility="visible" />
        <Constraint
            android:id="@+id/tvTitle"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@id/ivVector"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="40dp"
            android:visibility="visible" />
        <Constraint
            android:id="@+id/tvTitle"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@id/ivVector"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="40dp"
            android:visibility="visible" />
        <Constraint
            android:id="@+id/tvTitle"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@id/ivVector"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="40dp" />
        <Constraint
            android:id="@+id/tvSubtitle"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@id/tvTitle"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="16dp" />
        <Constraint
            android:id="@+id/tvSubtitle"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@id/tvTitle"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="16dp"
            android:visibility="visible" />
        <Constraint
            android:id="@+id/tvSubtitle"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@id/tvTitle"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="16dp"
            android:visibility="visible" />
        <Constraint
            android:id="@+id/btnGettingStarted"
            android:elevation="2dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="32dp"
            motion:layout_constraintTop_toBottomOf="@id/tvSubtitle" />
        <Constraint
            android:id="@+id/btnGettingStarted"
            android:elevation="2dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@id/tvSubtitle"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="32dp"
            android:visibility="visible" />
        <Constraint
            android:id="@+id/btnGettingStarted"
            android:elevation="2dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@id/tvSubtitle"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="32dp"
            android:visibility="visible" />
    </ConstraintSet>
</MotionScene>